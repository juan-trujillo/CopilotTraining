{
  "name": "fanhub",
  "version": "0.1.0",
  "private": true,
  "description": "A generic fan site for TV shows - needs theming!",
  "scripts": {
    "start": "docker-compose up",
    "start:build": "docker-compose up --build",
    "stop": "docker-compose down",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test": "echo 'No tests configured yet' && exit 0",
    "db:reset": "docker-compose down -v && docker-compose up -d db"
  },
  "keywords": ["fan-site", "tv-shows", "entertainment"],
  "author": "TechCorp Team",
  "license": "MIT"
}
